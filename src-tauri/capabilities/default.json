{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "opener:default",
    "sql:default",
    "sql:allow-load",
    "sql:allow-execute",
    "sql:allow-select",
    "sql:allow-close",
    "fs:default",
    "fs:allow-read-file",
    "fs:allow-write-file",
    "fs:allow-create",
    "fs:allow-mkdir",
    "core:window:allow-start-dragging"
  ]
}
